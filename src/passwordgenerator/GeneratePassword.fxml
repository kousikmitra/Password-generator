<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="228.0" prefWidth="375.0" style="-fx-background-color: #f0f0f0; -fx-border-width: 1; -fx-border-color: #000;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="passwordgenerator.GeneratePasswordController">
    <children>
        <Button id="generate" fx:id="generate" layoutX="141.0" layoutY="164.0" onAction="#handleButtonAction" style="-fx-background-color: #c1c1c1; -fx-border-color: #000000; -fx-border-width: 1;" text="Generate!" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="13.0" />
         </font></Button>
      <TextField id="password" fx:id="password" alignment="CENTER" editable="false" layoutX="56.0" layoutY="35.0" style="-fx-border-color: #000;" text="Password">
         <font>
            <Font size="20.0" />
         </font></TextField>
      <ComboBox fx:id="length" layoutX="26.0" layoutY="106.0" prefWidth="150.0" onAction="#test" promptText="Password Length" />
      <CheckBox fx:id="upperCase" layoutX="215.0" layoutY="88.0" mnemonicParsing="false" selected="true" text="Upper Case Letter" />
      <CheckBox fx:id="numbers" layoutX="215.0" layoutY="110.0" mnemonicParsing="false" selected="true" text="Numbers" />
      <CheckBox fx:id="specialChar" layoutX="215.0" layoutY="132.0" mnemonicParsing="false" text="Special Characters" />
    </children>
</AnchorPane>
